{
  "name": "azurekinect-node",
  "version": "0.0.1",
  "description": "Nodejs library to access the azure kinect data from the official SDK",
  "main": "www/kinect.js",
  "scripts": {
    "install": "node ./scripts/install.js && node-gyp rebuild",
    "clean": "node ./scripts/clean.js",
    "reinstall": "npm run clean && npm run install",
    "electron": "electron www/electron/electron.js",
    "rebuild": "node-gyp rebuild --debug && tsc && electron-rebuild",
    "e": "npm run electron",
    "r": "npm run rebuild",
    "t": "node www/test.js"
  },
  "license": "MIT",
  "dependencies": {
    "bindings": "^1.5.0",
    "download": "^7.1.0",
    "electron-rebuild": "^1.10.0",
    "extract-zip": "^1.6.7",
    "fs-extra": "^8.1.0",
    "jquery": "^3.4.1",
    "node-addon-api": "^1.0.0",
    "typescript": "^3.4.3"
  },
  "engines": {
    "node": ">= 10.6.0"
  },
  "devDependencies": {
    "@types/jquery": "^3.3.32",
    "@types/node": "^12.12.28",
    "electron": "^8.0.1",
    "nodemon": "^2.0.2",
    "rimraf": "^3.0.2"
  },
  "gypfile": true
}
